{
    "coc.preferences.hoverTarget": "float",
    "suggest.floatEnable": true,
    "suggest.maxPreviewWidth": 50,
    "suggest.enablePreview": true,
    "list.maxPreviewHeight": 50,
    "suggest.labelMaxLength": 50,
    "suggest.removeDuplicateItems": true,
    "javascript.suggest.autoImports": true,
    "java.import.gradle.enabled": true,
    "java.import.maven.enabled": true,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.import.gradle.wrapper.enabled": true,
    "java.autobuild.enabled": true,
    "java.format.enabled": true,
    "java.referencesCodeLens.enabled": true,
    "java.signatureHelp.enabled": true,
    "java.foldingRange.enabled": true,
    "java.trace.server": "verbose",
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.maven.downloadSources": true,
    "java.format.comments.enabled": true,
    "java.completion.guessMethodArguments": true,
    "java.format.onType.enabled": true,
    "java.codeGeneration.useBlocks": true,
    "explorer.previewAction.onHover": false,
    "explorer.file.column.indent.indentLine": true,
    "explorer.file.hiddenRules": {
      "extensions": [
        "o",
        "a",
        "obj",
        "pyc",
        "log",
        "iml",
        "jar",
        "original",
        "__pycache__",
        "bak",
        "dat",
        "dir"
      ],
      "filenames": [
        "target",
        ".env",
        "__pycache__"
      ],
      "patternMatches": [
        "^\\."
      ]
    },
    "coc.preferences.rootPatterns": [
        ".git",
        ".env",
        "src",
        "tests"
    ],
    "python.autoUpdateLanguageServer": true,
    "python.linting.enabled": true,
    "python.jediEnabled": false,
    "explorer.keyMappings": {
        "i": false,
        "gk": "expandablePrev",
        "gj": "expandableNext",
        "*": "toggleSelection",
        "<tab>": "actionMenu",
        "h": "collapse",
        "l": [
            "expandable?",
            "expand",
            "open"
        ],
        "J": [
            "toggleSelection",
            "nodeNext"
        ],
        "K": [
            "toggleSelection",
            "nodePrev"
        ],
        "gl": "expandRecursive",
        "gh": "collapseRecursive",
        "<2-LeftMouse>": [
            "expandable?",
            [
                "expanded?",
                "collapse",
                "expand"
            ],
            "open"
        ],
        "o": [
            "expanded?",
            "collapse",
            "expand"
        ],
        "<cr>": [
            "expandable?",
            "cd",
            "open"
        ],
        "e": "open",
        "s": "open:split",
        "S": "open:split:plain",
        "E": "open:vsplit",
        "t": "open:tab",
        "<bs>": "gotoParent",
        "gp": "preview:labeling",
        "y": "copyFilepath",
        "Y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",
        "a": "addFile",
        "A": "addDirectory",
        "r": "rename",
        ".": "toggleHidden",
        "R": "refresh",
        "?": "help",
        "q": "quit",
        "<esc>": "esc",
        "X": "systemExecute",
        "gd": "listDrive",
        "f": "search",
        "F": "searchRecursive",
        "gf": "gotoSource:file",
        "gb": "gotoSource:buffer",
        "[[": "sourcePrev",
        "]]": "sourceNext",
        "[d": "diagnosticPrev",
        "]d": "diagnosticNext",
        "[c": "gitPrev",
        "]c": "gitNext",
        "<<": "gitStage",
        ">>": "gitUnstage"
    }
}
